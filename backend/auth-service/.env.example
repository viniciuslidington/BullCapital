# Configurações do Banco de Dados
user=seu_usuario_db
password=sua_senha_db
host=localhost
port=5432
dbname=bull_capital_auth

# Configurações JWT
SECRET_KEY=sua_chave_secreta_super_segura_aqui
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30

# Configurações Google OAuth
# Obtenha estas credenciais no Google Cloud Console:
# 1. Acesse https://console.cloud.google.com/
# 2. Crie um novo projeto ou selecione um existente
# 3. Vá para "APIs e Serviços" > "Credenciais"
# 4. Clique em "Criar credenciais" > "ID do cliente OAuth 2.0"
# 5. Configure as URIs de redirecionamento autorizadas
GOOGLE_CLIENT_ID=seu_google_client_id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=seu_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:8001/api/v1/auth/google/callback

# Configurações CORS
ALLOWED_ORIGINS=["http://localhost:3000", "http://localhost:8000", "http://localhost:8001"]

# Debug
DEBUG_SQL=false
